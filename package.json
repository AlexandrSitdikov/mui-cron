{
  "name": "mui-cron",
  "version": "1.0.1",
  "description": "A React cron editor with MUI a forked repo from Itay Levy (https://github.com/levyitay/react-js-cron-mui)",
  "keywords": [
    "react",
    "reactjs",
    "js",
    "cron",
    "crontab",
    "editor",
    "widget",
    "generator",
    "mui"
  ],
  "homepage": "https://github.com/AlexandrSitdikov/mui-cron#readme",
  "bugs": {
    "url": "https://github.com/AlexandrSitdikov/mui-cron/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandrSitdikov/mui-cron.git"
  },
  "license": "MIT",
  "author": "Alexandr Sitdikov (https://github.com/AlexandrSitdikov)",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "del dist && yarn build:babel && yarn build:types",
    "build:babel": "concurrently \"yarn build:babel:esm\" \"yarn build:babel:cjs\"",
    "build:babel:cjs": "set BABEL_ENV=cjs && babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/cjs --presets=@babel/env",
    "build:babel:esm": "set BABEL_ENV=esm && babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/esm",
    "build:types": "tsc",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prettier": "prettier --write 'src/**/*.{ts?(x),css}'"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@storybook/addon-docs": "^7.6.20",
    "@storybook/addons": "^7.6.20",
    "@storybook/preset-create-react-app": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "concurrently": "^5.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "@mui/icons-material": "^6.4.10",
    "@mui/material": "^6.4.10"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@mui/material": ">=6.0.0",
    "fontsource-roboto": "^4.0.0"
  },
  "module": "dist/esm/index.js",
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
